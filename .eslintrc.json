{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "google",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-multi-spaces": "off",
    "require-jsdoc": "off",
    "max-len": [
      "error",
      {
        "code": 100,
        // Disable line length maximum for imports and URLs.
        "ignorePattern": "^import .*",
        "ignoreUrls": true
      }
    ]
  }
}
